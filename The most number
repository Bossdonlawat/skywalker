#include <stdio.h>
int main()
{
    int x,y,z;
    printf("Input three number : \n");
    scanf("%d %d %d",&x,&y,&z);
    if(x>=y && x>=z)
    {
        printf("The most number is %d",x);
    }
    else if(y>=x && y>=z)
    {
        printf("The most number is %d",y);
    }
    else if(z>=x && z>=y)
    {
        printf("The most number is %d",z);
    }
return 0;
}
